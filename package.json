{
  "name": "bonjour",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "db:drop": "dbmate drop",
    "db:create": "dbmate create",
    "db:up": "dbmate up",
    "db:down": "dbmate down",
    "db:test:drop": "dbmate -e TEST_DATABASE_URL drop",
    "db:test:create": "dbmate -e TEST_DATABASE_URL create",
    "db:test:load": "dbmate -e TEST_DATABASE_URL load",
    "db:test:setup": "npm run db:test:drop && npm run db:test:create && npm run db:test:load"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.0",
    "@types/bcrypt": "^5.0.2",
    "@types/pg": "^8.11.2",
    "bcrypt": "^5.1.1",
    "next": "14.1.3",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "sql-template-strings": "^2.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dbmate": "^2.12.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
